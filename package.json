{
  "name": "1000goals-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 192.168.1.178 --disable-host-check",
    "start-https": "ng serve --host 192.168.1.178 --ssl --ssl-key ./ssl/server.key --ssl-cert ./ssl/server.crt",
    "build": "ng build --output-path docs --base-href /1000goals/ ",
